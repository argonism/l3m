{
  "uid": "litellm-overview",
  "title": "LiteLLM Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "tags": ["litellm","prometheus","llm"],
  "refresh": "10s",
  "time": { "from": "now-6h", "to": "now" },
  "panels": [
    {
      "type": "stat",
      "title": "Requests / s",
      "gridPos": {"x":0,"y":0,"w":6,"h":4},
      "targets": [
        { "expr": "sum(rate(http_requests_total{job=Ä"litellmÄ"}[1m]))", "legendFormat": "RPS" }
      ]
    },
    {
      "type": "stat",
      "title": "Error Rate",
      "gridPos": {"x":6,"y":0,"w":6,"h":4},
      "targets": [
        { "expr": "sum(rate(http_requests_total{job=Ä"litellmÄ",status=~Ä"5..|4..Ä"}[5m])) / sum(rate(http_requests_total{job=Ä"litellmÄ"}[5m]))", "legendFormat": "errors" }
      ],
      "options": { "reduceOptions": {"calcs":["lastNotNull"]}, "colorMode": "value" }
    },
    {
      "type": "gauge",
      "title": "p95 Latency (s)",
      "gridPos": {"x":12,"y":0,"w":6,"h":4},
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=Ä"litellmÄ"}[5m])) by (le))" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p50 / p95 / p99 (s)",
      "gridPos": {"x":0,"y":4,"w":12,"h":8},
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{job=Ä"litellmÄ"}[5m])) by (le))", "legendFormat": "p50" },
        { "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=Ä"litellmÄ"}[5m])) by (le))", "legendFormat": "p95" },
        { "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=Ä"litellmÄ"}[5m])) by (le))", "legendFormat": "p99" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests by Status",
      "gridPos": {"x":12,"y":4,"w":12,"h":8},
      "targets": [
        { "expr": "sum by (status) (rate(http_requests_total{job=Ä"litellmÄ"}[1m]))", "legendFormat": "{{status}}" }
      ]
    },
    {
      "type": "bargauge",
      "title": "Requests by Path (top 10)",
      "gridPos": {"x":0,"y":12,"w":12,"h":8},
      "targets": [
        { "expr": "topk(10, sum by (handler) (rate(http_requests_total{job=Ä"litellmÄ"}[5m])))", "legendFormat": "{{handler}}" }
      ]
    },
    {
      "type": "bargauge",
      "title": "Requests by Model (if label exists)",
      "gridPos": {"x":12,"y":12,"w":12,"h":8},
      "targets": [
        { "expr": "topk(10, sum by (model) (rate(http_requests_total{job=Ä"litellmÄ"}[5m])))", "legendFormat": "{{model}}" }
      ],
      "description": "LiteLLMÇ™ÉÇÉfÉãñºÇlabelÇ…èoÇ∑èÍçáÇ…èWåvÇ≥ÇÍÇ‹Ç∑ÅBèoÇƒÇ¢Ç»ÇØÇÍÇŒãÛÇ…Ç»ÇËÇ‹Ç∑ÅB"
    }
  ]
}
